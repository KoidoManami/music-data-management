    "transfer:recent": "node src/transfer-recent-emails.js",
    "dev": "nodemon src/index.js"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "googleapis": "^126.0.1",
    "node-cron": "^3.0.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "music",
    "data-management",
    "gmail",
    "github",
    "automation",
    "issue-management"
